<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:controls="clr-namespace:DevToolbox.Wpf.Controls"
    xmlns:extensions="clr-namespace:DevToolbox.Wpf.MarkupExtensions"
    xmlns:system="clr-namespace:System;assembly=mscorlib"
    xmlns:theming="clr-namespace:DevToolbox.Wpf.Theming">
    <ResourceDictionary.MergedDictionaries>
        <theming:ThemePartResourceDictionary>
            <theming:ThemePartResourceDictionary.Key>
                <theming:ThemePartKeyExtension
                    AssemblyName="DevToolbox.Wpf"
                    Path="/Themes/Default/Generic.xaml"
                    ThemeName="Default" />
            </theming:ThemePartResourceDictionary.Key>
        </theming:ThemePartResourceDictionary>
        <theming:ThemePartResourceDictionary>
            <theming:ThemePartResourceDictionary.Key>
                <theming:ThemePartKeyExtension
                    AssemblyName="DevToolbox.Wpf"
                    Path="/Themes/Dark/Generic.xaml"
                    ThemeName="Dark" />
            </theming:ThemePartResourceDictionary.Key>
        </theming:ThemePartResourceDictionary>
        <theming:ThemePartResourceDictionary>
            <theming:ThemePartResourceDictionary.Key>
                <theming:ThemePartKeyExtension
                    AssemblyName="DevToolbox.Wpf"
                    Path="/Themes/Light/Generic.xaml"
                    ThemeName="Light" />
            </theming:ThemePartResourceDictionary.Key>
        </theming:ThemePartResourceDictionary>

        <ResourceDictionary Source="{extensions:AssemblyResourceExtension Themes/Styles/WindowEx.xaml}" />
        <ResourceDictionary Source="{extensions:AssemblyResourceExtension Themes/Styles/TabsWindow.xaml}" />
        <ResourceDictionary Source="{extensions:AssemblyResourceExtension Themes/Styles/SliderWindow.xaml}" />

        <ResourceDictionary Source="{extensions:AssemblyResourceExtension Themes/Styles/DropDown.xaml}" />
        <ResourceDictionary Source="{extensions:AssemblyResourceExtension Themes/Styles/EditBox.xaml}" />
        <ResourceDictionary Source="{extensions:AssemblyResourceExtension Themes/Styles/ExpanderEdit.xaml}" />
        <ResourceDictionary Source="{extensions:AssemblyResourceExtension Themes/Styles/FontGlyph.xaml}" />
        <ResourceDictionary Source="{extensions:AssemblyResourceExtension Themes/Styles/NumberBox.xaml}" />
        <ResourceDictionary Source="{extensions:AssemblyResourceExtension Themes/Styles/ProgressRing.xaml}" />
        <ResourceDictionary Source="{extensions:AssemblyResourceExtension Themes/Styles/SliderControl.xaml}" />
        <ResourceDictionary Source="{extensions:AssemblyResourceExtension Themes/Styles/SliderPanel.xaml}" />
        <ResourceDictionary Source="{extensions:AssemblyResourceExtension Themes/Styles/SpeechBalloon.xaml}" />
        <ResourceDictionary Source="{extensions:AssemblyResourceExtension Themes/Styles/TabControlEdit.xaml}" />
        <ResourceDictionary Source="{extensions:AssemblyResourceExtension Themes/Styles/TabItemEdit.xaml}" />
        <ResourceDictionary Source="{extensions:AssemblyResourceExtension Themes/Styles/TreeViewEdit.xaml}" />
        <ResourceDictionary Source="{extensions:AssemblyResourceExtension Themes/Styles/TreeViewItemEdit.xaml}" />
        <ResourceDictionary Source="{extensions:AssemblyResourceExtension Themes/Styles/WaitingIndicator.xaml}" />
    </ResourceDictionary.MergedDictionaries>

    <FontFamily x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type controls:FontGlyph}, ResourceId=FontFamilyKey}">
        pack://application:,,,/DevToolbox.Wpf;component/SymbolThemeFont/fonts/#SymbolThemeFont
    </FontFamily>

    <system:Double x:Key="FontSize8">8</system:Double>
    <system:Double x:Key="FontSize9">9</system:Double>
    <system:Double x:Key="FontSize10">10</system:Double>
    <system:Double x:Key="FontSize11">11</system:Double>
