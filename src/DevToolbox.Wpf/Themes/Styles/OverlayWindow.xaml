    <Style TargetType="{x:Type windows:OverlayWindow}">
        <Setter Property="ShowInTaskbar" Value="False" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type windows:OverlayWindow}">
                    <AdornerDecorator>
                        <Grid UseLayoutRounding="True">
                            <ContentControl
                                x:Name="PART_AbsolutleOptions"
                                ContentTemplate="{StaticResource AbsolutleOptions}"
                                Visibility="{Binding RelativeSource={RelativeSource Mode=TemplatedParent}, Path=IsAbsolutleOptionsVisible, Converter={StaticResource BooleanToVisibilityConverter}}" />
                            <Canvas Visibility="{Binding RelativeSource={RelativeSource Mode=TemplatedParent}, Path=IsInnerOptionsVisible, Converter={StaticResource BooleanToVisibilityConverter}}">
                                <ContentControl x:Name="PART_Options" ContentTemplate="{StaticResource RelativeOptions}" />
                            </Canvas>
                        </Grid>
                    </AdornerDecorator>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsAbsolutleOptionsVisible" Value="True">
                            <Setter TargetName="PART_Options" Property="ContentTemplate" Value="{StaticResource MultipleOptions}" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

</ResourceDictionary>