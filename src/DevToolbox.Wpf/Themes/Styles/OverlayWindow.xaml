    <DataTemplate x:Key="AbsolutleOptions">
        <Grid Opacity="0.6">
            <controls:OverlayButton
                Width="42"
                Height="42"
                Margin="6,0,0,0"
                HorizontalAlignment="Left"
                Background="{StaticResource DockLeftOuterBorder}"
                DockingPosition="Left"
                Style="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=windows:OverlayWindow}, Path=DockManager.OverlayButtonStyle}" />
            <controls:OverlayButton
                Width="42"
                Height="42"
                Margin="0,0,6,0"
                HorizontalAlignment="Right"
                Background="{StaticResource DockRightOuterBorder}"
