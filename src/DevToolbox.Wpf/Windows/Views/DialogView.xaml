<views:BaseDialogView
    x:Class="DevToolbox.Wpf.Windows.Views.DialogView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:viewModels="clr-namespace:DevToolbox.Wpf.Windows.ViewModels"
    xmlns:views="clr-namespace:DevToolbox.Wpf.Windows.Views"
    d:DataContext="{d:DesignInstance viewModels:DialogViewModel}"
    d:DesignWidth="458"
    mc:Ignorable="d">
    <ContentControl
        Content="{Binding DataContext, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=views:DialogView}}"
        Focusable="False"
        IsTabStop="False" />
</views:BaseDialogView>
