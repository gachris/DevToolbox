<Application
    x:Class="DevToolbox.Wpf.Demo.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:demo="clr-namespace:DevToolbox.Wpf.Demo"
    StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/DataTemplates/ExampleGategoryView.xaml" />
                <ResourceDictionary Source="/DataTemplates/ExampleSubGategoryView.xaml" />
                <ResourceDictionary Source="/DataTemplates/ExampleView.xaml" />
                <ResourceDictionary Source="/DataTemplates/PropertyView.xaml" />
                <ResourceDictionary Source="/DataTemplates/SettingsView.xaml" />
            </ResourceDictionary.MergedDictionaries>

            <demo:ViewModelLocator x:Key="ViewModelLocator" />

        </ResourceDictionary>
    </Application.Resources>

</Application>
