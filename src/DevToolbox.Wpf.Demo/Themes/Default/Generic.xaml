<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="TextBlock.Primary.Foreground" Color="#1E1E1E" />

    <!--#region ExampleCategory-->

    <SolidColorBrush x:Key="ExampleCategory.Background" Color="#F0F0F0" />
    <SolidColorBrush x:Key="ExampleCategory.BorderBrush" Color="#CACACA" />
    <SolidColorBrush x:Key="ExampleCategory.Foreground" Color="#000000" />

    <!--#endregion-->

    <!--#region Example-->

    <SolidColorBrush x:Key="Example.Background" Color="#F3F3F3" />
    <SolidColorBrush x:Key="Example.BorderBrush" Color="#EAEAEA" />
    <SolidColorBrush x:Key="Example.Foreground" Color="#1A1A1A" />
    <SolidColorBrush x:Key="Example.Properties.Background" Color="#FBFBFB" />

    <!--#endregion-->

    <!--#region XamlEditorView-->

    <SolidColorBrush x:Key="XamlEditorView.Background" Color="#FDFDFD" />
    <SolidColorBrush x:Key="XamlEditorView.BorderBrush" Color="#EAEAEA" />
    <SolidColorBrush x:Key="XamlEditorView.TextEditor.Background" Color="Transparent" />

    <!--#endregion-->

    <!--#region SettingsView-->

    <SolidColorBrush x:Key="SettingsView.Background" Color="#F0F0F0" />
    <SolidColorBrush x:Key="SettingsView.BorderBrush" Color="#CACACA" />

    <!--#endregion-->

    <!--#region Button-->

    <SolidColorBrush x:Key="Button.Borderless.Background" Color="Transparent" />
    <SolidColorBrush x:Key="Button.Borderless.MouseOverBackground" Color="Transparent" />
    <SolidColorBrush x:Key="Button.Borderless.PressedBackground" Color="Transparent" />
    <SolidColorBrush x:Key="Button.Borderless.DisabledBackground" Color="Transparent" />

    <SolidColorBrush x:Key="Button.Primary.Foreground" Color="#167EFB" />
    <SolidColorBrush x:Key="Button.Primary.MouseOverForeground" Color="#167EFB" />
    <SolidColorBrush x:Key="Button.Primary.PressedForeground" Color="#094E96" />
    <SolidColorBrush x:Key="Button.Primary.DisabledForeground" Color="#4D4D4D" />

    <!--#endregion-->

</ResourceDictionary>